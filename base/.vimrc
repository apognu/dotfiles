set term=gnome-256color

colorscheme Tomorrow-Night

" Global

set number
set background=dark
set ignorecase
set hlsearch incsearch
set autoindent
set display=lastline
set cursorline
set history=1000
set nobackup

" Tabs settings

set expandtab
set shiftwidth=2
set tabstop=2
set softtabstop=2
set backspace=indent,eol,start

filetype indent on
syntax on

set regexpengine=1

set t_ut=
